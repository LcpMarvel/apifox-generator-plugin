{
  "name": "apifox-generator-plugin",
  "version": "2.0.0",
  "description": "Claude Code Plugin for generating API clients from Apifox",
  "type": "module",
  "workspaces": [
    "skills/*"
  ],
  "scripts": {
    "build": "npm run build --workspace=@apifox-generator/core",
    "clean": "npm run clean --workspace=@apifox-generator/core",
    "dev": "npm run dev --workspace=@apifox-generator/core",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude-code",
    "plugin",
    "apifox",
    "openapi",
    "codegen",
    "api-client"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LcpMarvel/apifox-generator-plugin"
  },
  "bugs": {
    "url": "https://github.com/LcpMarvel/apifox-generator-plugin/issues"
  },
  "homepage": "https://github.com/LcpMarvel/apifox-generator-plugin#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    ".claude-plugin/",
    "commands/",
    "agents/",
    "templates/",
    "skills/",
    "README.md",
    "CHANGELOG.md",
    "schema.json"
  ]
}
